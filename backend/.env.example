# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL (for OAuth redirects)
FRONTEND_URL=https://codesight.netlify.app

# back end URL (for OAuth redirects)
BACKEND_URL=https://codesight-production.up.railway.app

# Session Secret (change this to a random string in production)
SESSION_SECRET=your-random-secret-key-change-in-production

# GitHub OAuth Configuration
# Get these from: https://github.com/settings/developers
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GITHUB_CALLBACK_URL=https://codesight-production.up.railway.app/api/auth/github/callback

# Gemini API Key (for AI features)
GEMINI_API_KEY=your_gemini_api_key_here


